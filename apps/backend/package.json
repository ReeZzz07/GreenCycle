{
  "name": "backend",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate src/migrations/$npm_config_name -d src/database/typeorm.config.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/database/typeorm.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/database/typeorm.config.ts",
    "check-user": "ts-node scripts/check-user.ts"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.2.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.2.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.0",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.2",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^10.0.0",
    "bcrypt": "^5.1.1",
    "cache-manager": "^7.2.5",
    "cache-manager-ioredis-yet": "^2.1.2",
    "cache-manager-redis-store": "^3.0.1",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "ioredis": "^5.8.2",
    "joi": "^17.10.1",
    "nestjs-pino": "^4.4.1",
    "nodemailer": "^7.0.10",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pdfmake": "^0.2.20",
    "pg": "^8.11.1",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.2.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.9.0",
    "@types/nodemailer": "^7.0.3",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.9",
    "@types/pdfmake": "^0.2.12",
    "@types/xlsx": "^0.0.36",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}
